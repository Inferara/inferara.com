{
  "ci": {
    "collect": {
      "staticDistDir": "./public",
      "url": [
        "http://localhost/en/index.html",
        "http://localhost/jp/index.html",
        "http://localhost/en/services/index.html",
        "http://localhost/jp/services/index.html",
        "http://localhost/en/demo/index.html",
        "http://localhost/jp/demo/index.html",
        "http://localhost/en/use-cases/amm/index.html"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.9}],
        "categories:accessibility": ["error", {"minScore": 0.95}],
        "categories:best-practices": ["error", {"minScore": 0.95}],
        "categories:seo": ["error", {"minScore": 0.9}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
